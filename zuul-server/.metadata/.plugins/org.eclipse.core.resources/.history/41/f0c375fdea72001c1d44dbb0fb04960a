server:
  port: 9000
  
eureka:
  client:
    serviceUrl:
      defaultZone: http://localhost:8761/eureka
  instance:
    prefer-ip-address: true
  
spring:
  application:
    name: api-gateway
  cloud:
    gateway:
      routes:
      - id: eureka-client1
        uri: lb://eureka-client1
          #http://localhost:8122/
        predicates:
        - Path=/eureka-client1/**
      - id: eureka-client2
        uri: lb://eureka-client2
          #http://localhost:8122/
        predicates:
        - Path=/eureka-client2/** 
        